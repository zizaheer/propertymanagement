<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title> Karez - Property Management System </title>
    <!--[if IE]><meta http-equiv='X-UA-Compatible' content='IE=edge,chrome=1'><![endif]-->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,400i,600,700" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Montserrat:300,400,500,600,700,900,900i" rel="stylesheet">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <link rel="stylesheet" href="Contents/css/bootstrap.min.css" media="all">
    <link rel="stylesheet" href="Contents/css/boxicons.min.css" media="all">
    <link rel="stylesheet" href="Contents/css/uikit.min.css" media="all">
    <link rel="stylesheet" href="Contents/css/style.css" media="all">
    <link rel="stylesheet" href="Contents/css/responsive.css" media="all">
</head>
<body>


    <!-- header section -->
    <header class="tp-main-menu header-menu-1 sticky-header">
        <div class="container">
            <div class="row row_menu_3">
                <div class="col-lg-2 col-md-12 col-sm-12 col-logo-1">
                    <div class="tagpoint-wrap-logo lwl_header_layout_1">
                        <a href="home" title="Karez Logo">
                            <img src="Contents/images/logos/karez-logo-v1.png" alt="Karez">
                        </a>
                        <span class="phone_menu phone_menu_2 p_color">
                            <i class='bx bx-menu'></i>
                        </span>
                    </div>
                </div>
                <div class="col-lg-10 col-md-12 col-sm-12">
                    <nav class="tp-menu tp-menu-1">
                        <ul class="tagpoint-main-menu">
                            <li class="current_page_item  active p_color has-sub ">
                                <i class="bx bx-chevron-down tp_phone_dropdown"></i>
                                <a title="Home" href="Home">
                                    <i class="bx bx-chevron-down"></i>
                                    Home
                                </a>
                            </li>
                            <li class="has-sub">
                                <i class="bx bx-chevron-down tp_phone_dropdown"></i>
                                <a title="About Us" href="About">
                                    <i class="bx bx-chevron-down"></i>
                                    About us
                                </a>
                            </li>
                            <li class="has-sub">
                                <i class="bx bx-chevron-down tp_phone_dropdown"></i>
                                <a title="Services" href="Services">
                                    <i class="bx bx-chevron-down"></i>
                                    Services
                                </a>
                            </li>
                            <li class="has-sub">
                                <i class="bx bx-chevron-down tp_phone_dropdown"></i>
                                <a title="Projects" href="Projects">
                                    <i class="bx bx-chevron-down"></i>
                                    Projects
                                </a>
                            </li>
                            <li>
                                <i class="bx bx-chevron-down tp_phone_dropdown"></i>
                                <a title="Services" href="Contact">
                                    Contact us
                                </a>
                            </li>
                            <li class="wrap_search tp_csq  search_hlayout_1">
                                <i class='bx tp-seachicon-icon bx-search'></i>
                                <div class="search-content search-content_1 ">
                                    <div class="tp_close_search_1">
                                        <i class='bx bx-x  tp_close_search_icon'></i>
                                    </div>
                                    <form class="tagpoint-nav-search" id="tagpoint-navsearch" action="#">
                                        <input type="text" name="s" value="" placeholder="Search..." id="lns-search">
                                        <button class="lns-button" type="submit">Search</button>
                                    </form>
                                </div>
                            </li>
                        </ul>
                    </nav>
                </div>
            </div>
        </div>
    </header>

    <!-- header bg-image breadcrumbs section 2  -->
    <section class="tagpoint_row_section breadcrubs_section breadcrumbs_3">
        <div class="container">
            <div class="row">
                <div class="col-12">
                    <div class="wrap_breadcrumbs_col">
                    </div>
                </div>
            </div>
        </div>
    </section>

    <div>
        @RenderBody()
    </div>

    <footer class="tp_footer ">
        <div class="container tagpoint_container">
            <div class="footer_wrap_nav">
                <ul class="footer_navbar">
                    <li><a href="#">About</a></li>
                    <li><a href="#">Services</a></li>
                    <li><a href="#">Portfolio</a></li>
                    <li><a href="#">Conact us</a></li>
                </ul>
            </div>
            <div class="row copywright_row">
                <div class="col-lg-7 col-md-7 col-12 ">
                    <p class="copywright">
                        Copyright © 2018
                    </p>
                </div>
                <div class="col-5 col-md-5 col-12">
                    <ul class="privacy_list tp_algin_right">
                        <li><a href="#">Privacy Policy</a>|</li>
                        <li><a href="#">Cookie Policy</a>|</li>
                        <li><a href="#">Terms & Conditions</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </footer>
    <div class="tp-per-load"></div>
   
    
    <script src="Contents/js/jquery.min.js"></script>
    <script src="Contents/js/uikit.min.js"></script>
    <script src="Contents/js/odometer.min.js"></script>
    <script src="Contents/js/jquery.appear.min.js"></script>
    <script src="Contents/js/index.js"></script>
</body>
</html>
